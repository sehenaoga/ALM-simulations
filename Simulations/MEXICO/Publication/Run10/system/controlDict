FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

//application     simpleFoam;
application     uRANSABLWFarmSolver;

//Time Control

startFrom       firstTime;//startTime;//latestTime;

startTime       0;

stopAt          endTime;

endTime         15.1; //To guaranee it also writes near the last timestep: 15.0024

deltaT          7e-4;

//Data writing

writeControl    timeStep; //adjustableRunTime;

writeInterval   3572; //Writes each 2.5s

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression compressed;

timeFormat      general;

timePrecision   6;

//Adjustable time step

ajustTimeStep    no;

//maxCo             0.75;

//maxDeltaT         25.0;

//Data reading

runTimeModifiable no;

startTimeMean   5;  // Starts averaging quantities from t = 5s

startTimeCorr  0.3;


